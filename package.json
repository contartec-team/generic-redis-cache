{
  "name": "@contartecTeam/generic-redis-cache",
  "version": "0.1.0",
  "description": "Generic service for operations on redis keys",
  "main": "lib/services/GenericRedisCache.js",
  "scripts": {
    "lint": "cross-env NODE_ENV=test eslint -c ./.eslintrc.json \"lib/**/*.js\" \"tests/**/*.js\"",
    "docs": "documentation build lib/** -f html -o docs",
    "mocha": "cross-env NODE_ENV=test _mocha --opts mocha.opts",
    "test": "cross-env TZ=UTC NODE_ENV=test npm run lint && npm run test:coverage && npm run coverage:upload",
    "test:watch": "cross-env TZ=UTC NODE_ENV=test npm run lint && npm run mocha -- --watch",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=text-summary --reporter=lcov --reporter=cobertura --report-dir=./coverage --reporter=html npm run mocha -- --reporter=min --exit",
    "coverage:upload": "codecov && npm run coverageBadge",
    "coverageBadge": "istanbul-cobertura-badger -f -v -b badge",
    "update:docs": "npm run docs && git add docs/. && git diff-index --quiet HEAD || git commit -m 'Updating `docs`'",
    "postinstall": "link-module-alias"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://contartecTeam@github.com/contartecTeam/generic-redis-cache.git"
  },
  "keywords": [
    "node",
    "js",
    "redis",
    "cache"
  ],
  "author": "contartec",
  "license": "MIT",
  "_moduleAliases": {
    "generic-redis-cache-lib": "./lib",
    "generic-redis-cache-services": "./lib/services",
    "generic-redis-cache-enums": "./lib/enums",
    "generic-redis-cache-configs": "./lib/configs",
    "generic-redis-cache-types": "./lib/services/types",
    "generic-redis-cache-doc": "./docs",
    "generic-redis-cache-tests": "./tests",
    "generic-redis-cache-mocks": "./tests/mocks"
  },
  "dependencies": {
    "dotenv": "^1.2.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "chai-things": "^0.2.0",
    "documentation": "^11.0.0",
    "deep-diff": "^1.0.2",
    "deep-equal": "^1.1.1",
    "jsdoc": "^3.6.2"
  },
  "devDependencies": {
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-datetime": "^1.4.1",
    "chai-http": "^2.0.1",
    "co": "^4.6.0",
    "co-express": "^1.2.1",
    "co-mocha": "^1.1.3",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "deep-equal-in-any-order": "^1.0.27",
    "eslint": "^5.16.0",
    "faker": "^3.1.0",
    "husky": "^3.0.5",
    "istanbul-cobertura-badger": "^1.3.0",
    "link-module-alias": "^1.2.0",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "redis": "^2.8.0",
    "redis-rejson": "^1.0.0",
    "redis-url": "^1.2.1",
    "rewire": "^2.5.2",
    "sinon": "^1.17.5",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0",
    "socket.io": "^1.6.0",
    "socket.io-redis": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run update:docs"
    }
  },
  "nyc": {
    "exclude": [
      "*.js",
      "tests",
      "enums"
    ]
  }
}
